{
    "compilerOptions": {
        "target": "es6",
        "module": "commonjs",
        "esModuleInterop": true,
        "strict": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "outDir": "dist",
        "noEmit": false,
        "sourceMap": true,
    },
    "include": [
        "./src/**/*",
        "test/**/*",
    ],
    "exclude": [
        "node_modules"
    ]
}